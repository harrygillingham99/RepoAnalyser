{
  "name": "front-end",
  "version": "0.1.0",
  "author": "Harry Gillingham",
  "description": "A GitHub Repository Analyser",
  "main": "electron/main.js",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.1.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.2",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.19.15",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@types/react-router-dom": "^5.1.7",
    "bootstrap": "^4.6.0",
    "concurrently": "^5.3.0",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.3",
    "react-bootstrap-icons": "^1.3.0",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-use": "^17.1.0",
    "typescript": "^4.1.3",
    "unstated-next": "^1.1.0",
    "web-vitals": "^0.2.4"
  },
  "build": {
    "appId": "RepoAnalyser",
    "productName": "repo-analyser",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ]
  },
  "devDependencies": {
    "craco-alias": "^2.1.1",
    "cross-env": "^7.0.2",
    "electron": "^11.2.3",
    "electron-builder": "^22.9.1",
    "husky": "^5.0.8",
    "lint-staged": "^10.5.3",
    "nswag": "^13.10.1",
    "pinst": "^2.1.4",
    "prettier": "2.2.1"
  },
  "scripts": {
    "start": "craco start --verbose",
    "build": "cross-env NODE_ENV=production && craco build",
    "build-dev": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "generate-client": "nswag run ./backend.nswag /runtime:WinX64",
    "postinstall": "husky install",
    "pre-commit": "lint-staged --verbose",
    "start-electron": "concurrently \"set BROWSER=none && npm start\" \"set ELECTRON_START_URL=http://localhost:3000 && electron . \"",
    "package-electron": "npm run build && npm run build-electron && electron-builder build -c.extraMetadata.main=build/electron/main.js --publish never",
    "package-electron-dev": "npm run build-dev && npm run build-electron && electron-builder build -c.extraMetadata.main=build/electron/main.js --publish never",
    "build-electron": "mkdir build\\src && Xcopy /E /I /Y electron build\\electron"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,css,md,ts,tsx}": "prettier --write"
  }
}
