{
  "name": "front-end",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.1.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.2",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.19.15",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "typescript": "^4.1.3",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "husky": "^5.0.8",
    "lint-staged": "^10.5.3",
    "nswag": "^13.10.1",
    "pinst": "^2.1.4",
    "prettier": "2.2.1"
  },
  "scripts": {
    "start": "craco start --verbose",
    "build": "cross-env NODE_ENV=production && craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "generate-client": "nswag run ./backend.nswag /runtime:WinX64",
    "postinstall": "husky install",
    "pre-commit": "lint-staged"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,css,md,ts,tsx}": "prettier --write"
  }
}
